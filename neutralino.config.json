{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/solid-src/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "filesystem.readDirectory"
  ],
  "modes": {
    "window": {
      "title": "myapp",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/solid-src/dist/assets/favicon-0e726a38.ico"
    }
  },
  "cli": {
    "binaryName": "myapp",
    "resourcesPath": "/solid-src/dist",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.2",
    "frontendLibrary": {
      "patchFile": "/solid-src/dist/index.html",
      "devUrl": "http://localhost:3000"
  }
  }
}